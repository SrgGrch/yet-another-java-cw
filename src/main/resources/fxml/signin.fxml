<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<StackPane xmlns="http://javafx.com/javafx/8.0.172-ea" fx:id="signIn_stack" xmlns:fx="http://javafx.com/fxml/1" fx:controller="newsline.ui.SignInController">
    <children>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="300.0">
            <children>
                <Label text="Лента новостей">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                    <font>
                        <Font size="28.0" />
                    </font>
                </Label>
                <TextField fx:id="signIn_login" promptText="Логин">
                    <VBox.margin>
                        <Insets left="16.0" right="16.0" top="48.0" />
                    </VBox.margin>
                </TextField>
                <PasswordField fx:id="signIn_password" promptText="Пароль">
                    <VBox.margin>
                        <Insets left="16.0" right="16.0" top="8.0" />
                    </VBox.margin>
                </PasswordField>
                <Button fx:id="signIn_auth" mnemonicParsing="false" text="Войти">
                    <VBox.margin>
                        <Insets top="8.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="signIn_reg" mnemonicParsing="false" text="Еще нет аккаунта?">
                    <VBox.margin>
                        <Insets top="8.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>
</StackPane>
